   "source": [
    "# checking survivor numbers fopr various colum heading in the dataframe.\n",
    "#creating list of colums for which we need to check survived data\n",
    "\n",
    "cols = ['who', 'sex', 'sibsp', 'parch', 'embarked', 'pclass']\n",
    "\n",
    "# defining the number of rows and colums we want to view the graphs on. 4 graphs will be generated\n",
    "\n",
    "n_rows = 2\n",
    "n_cols = 3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 691.2x460.8 with 6 Axes>"
      ]
